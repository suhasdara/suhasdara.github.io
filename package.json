{
  "name": "astro-portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "node copy-assets.js && astro dev",
    "start": "astro dev",
    "build": "node copy-assets.js && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "git stash -u && npm run build && gh-pages -d dist -b master --dotfiles && git stash pop",
    "push": "git add . && git commit -m 'Update portfolio' && git push origin main"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.3",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.2",
    "@astrojs/tailwind": "^5.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "astro": "^5.0.0",
    "lucide-astro": "^0.536.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2"
  }
}
